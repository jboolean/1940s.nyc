{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "controllerPathGlobs": ["src/**/*Controller.ts"],
  "spec": {
    "outputDirectory": "tsoa-build",
    "specVersion": 3,
    "securityDefinitions": {
      "netlify": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "routes": {
    "routesDir": "tsoa-build",
    "authenticationModule": "src/api/auth/authHandler.ts"
  }
}